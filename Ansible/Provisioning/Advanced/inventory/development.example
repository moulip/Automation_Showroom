# Example Inventory for Development Environment
#
# Copy and customize:
#   cp inventory/development.example inventory/development

[wallix_bastions]
bastion-dev ansible_host=bastion-dev.example.com

[wallix_bastions:vars]
ansible_connection=local
ansible_python_interpreter=python3
wallix_api_port=443
wallix_api_ssl=true

[development:children]
wallix_bastions

[development:vars]
deployment_environment=development
